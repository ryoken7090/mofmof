<p id="notice"><%= notice %></p>

<h1>物件一覧</h1>

<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>家賃</th>
      <th>住所</th>
      <th>築年数</th>
      <th>備考</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.rent %></td>
        <td><%= room.address %></td>
        <td><%= room.age %></td>
        <td><%= room.comment %></td>
        <td><%= link_to 'Show', room %></td>
        <td><%= link_to 'Edit', edit_room_path(room) %></td>
        <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: '消去していいですか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to '物件を登録', new_room_path %>
